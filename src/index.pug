extends partials/layout

block vars
	- var title = "GREATEST page ever"
	- var navigation = "index"

block content
	.blackshadow
	.hero-section.app-full-page
		.app-page-bg
			.hero-section__bg
				img(src="img/bg/hero-bg.png", alt="").bg-image
		.container
			.main-logo
				a(href="#", title="title")
					img(src="img/icons/main-logo.svg", alt="Money Token")
			.hero-section__sub-header.
				MoneyToken
			.hero-section__main-header.
				Smart loans backed 
				#[br]
				by crypto assets.
			.hero-section__button-group
				a.hero-section__button.app-button.app-button_primer(href="#applyforpresale") Appy for pre sale
				a.hero-section__button.app-button.app-button_primer(href="#signin") Sign in
			.hero-section__button-group
				a.hero-section__button.hero-section__button_image.app-button.app-button_transparent.app-button_image.app-button_image_pdf(href="#whitepaper") Whitepaper
				a.hero-section__button.hero-section__button_image.app-button.app-button_transparent.app-button_image.app-button_image_pdf(href="#onepager") Onepager
				a.hero-section__button.hero-section__button_image.app-button.app-button_transparent.app-button_image.app-button_image_github(href="#github") Github

	.intro-video-section.app-full-page
		.container
			.intro-video-section__video-holder
				.intro-video-section__video
					video#main_vid(poster='img/content/video-poster.jpg')
						source(src='video/money_token.mp4', type='video/mp4; codecs="avc1.42E01E, mp4a.40.2"')
				.intro-video-section__button.app-button.app-button_primer Play Video

	.cash-section.app-full-page
		.container
			.cash-section__content
				.cash-section__title Get cash in any currency anytime and anywhere 
				ul.cash-section__list
					li.cash-section__list-item Card balance in USD 
					li.cash-section__list-item Fast and easy loan cash-in
					li.cash-section__list-item Repay your loan
				.cash-section__image
					img(src="img/content/credit-cards.png", alt="")
				a(href="#", title="title").cash-section__button.app-button.app-button_primer Preorder	

	.slider-section.app-full-page
		.container
			.slider-section__title MoneyToken
			span.slider-section__title-small â€”  platform for short term loans in stable currencies with high growth crypto assets as a collateral.
